{
  "name": "jump",
  "private": true,
  "description": "GitHub Dashboard",
  "author": "Brett Buddin",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/brettbuddin/jump"
  },
  "scripts": {
    "clean":  "rm -rf build/ && rm -rf dist/",
    "prebuild": "npm run clean",
    "build": "webpack",
    "predeploy": "npm run clean && webpack --optimize-minimize --output-path=dist/",
    "deploy": "rsync -chavzOP --stats --group=www-data dist/ bubbles:/usr/share/nginx/jump/",
    "start": "webpack-dev-server"
  },
  "dependencies": {
    "moment": "2.11.2",
    "mousetrap": "^1.5.3",
    "react": "~0.14.7",
    "react-addons-linked-state-mixin": "0.14.7",
    "react-dom": "~0.14.7",
    "underscore": "1.8.3",
    "whatwg-fetch": "^0.11.0"
  },
  "devDependencies": {
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.1",
    "exports-loader": "^0.6.3",
    "fetch": "^1.0.1",
    "file-loader": "^0.8.5",
    "imports-loader": "^0.6.5",
    "node-sass": "^3.4.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "uglify-loader": "^1.3.0",
    "urijs": "^1.17.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
}
